{
    "phantomjs": {
        "args": ["--ignore-ssl-errors=true"]
    },
    "message_queue": "redis://localhost:6379/0",
    "webui": {
        "port": 5000
    },
    "rate_limit": {
        "global": {
            "requests_per_minute": 60
        },
        "per_domain": {
            "example.com": {
                "requests_per_minute": 10
            },
            "httpbin.org": {
                "requests_per_minute": 5
            },
            "gnavi.co.jp": {
                "requests_per_minute": 5
            },
            "amazon.co.jp": {
                "requests_per_minute": 2
            }
        }
    }
}
